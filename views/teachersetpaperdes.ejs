<% include partials/header %>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
</head>
<h1>Welcome <%= uname%></h1>
<h3>Your paper id is <%= testid %></h3>

<form action="/teachersetpaper/<%= testid %>/desqp" method="post"><br>

<!--<label>Question no -->
<!--<input type="text" name="tnfquestionno" ></label><br>-->
<label>Question  
<input type="text" name="desquestion" ></label><br>
<label>Constant Keywords  
<input type="text" name="desckw" placeholder="enter keywords seperated by comma"  title="enter keywords seperated by comma" ></label>
<label>Weithage
<input type="text" name="desckwweightage" ></label>
  <br>
<input type="submit" style="float:left" value="Submit">

    </form>
                    <div class="w3-container">
  <h2>Descriptive set by you</h2>
  
  <table class="w3-table-all">
    <thead>
      <tr class="w3-red"> <th>Question</th> <th>ckw(T/F)</th>  
      <th>Weightage</th>  </tr>
    </thead>
    <% allqps.forEach(function(allqps){ %>
    <tr><td>
    <%= allqps.desquestion %></td><td><%= allqps.desckw %></td>
          <td><%= allqps.desckwweightage %></td>
    </tr>
    <% }); %>
  </table>
</div>


                  
                

